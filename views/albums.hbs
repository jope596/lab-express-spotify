<link rel="stylesheet" href="/styles/styleAlbums.css">

<div class="albums">
  {{#each albums}}
    <div class="albums-div">
      <img src="{{this.images.1.url}}" alt="{{this.name}}" />
    
    <p>
      {{this.name}}
    </p>

<a href="/tracks/{{this.id}}">View Tracks</a>
  </div>
  {{/each}}
</div>